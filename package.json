    "chai": "^4.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "jest-diff": "^26.4.2",
    "jest-junit": "^11.1.0",
    "solium": "^1.2.5",
    "solparse": "^2.2.8"
  },
  "dependencies": {
    "@eth-optimism/smock": "^0.2.1-alpha.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.0",
    "dotenv": "^8.2.0",
